.ATAWindow {
	position:fixed;
	border-radius:3px;
	border:1px solid #49505740;
	//background-color:#1A1A23FF;
	background-color:#1A1A1AFF;
	//background-image:url("../images/idealdata-logo-02.png");
	background-repeat:no-repeat;
	background-attachment:fixed;
	background-position:center;
	color:white;
	//backdrop-filter:blur(5px);
	box-shadow:2px 1px 5px 1px #1A1A1A97;
}

.ATAWindow .Title {
	width:100%;
	top:0;
	left:0;
	font-size:0.9em;
	position:absolute;
	background-color:#00010070;
	background-image:linear-gradient(#1A1A1AFF, #00010080);
	color:#F0F0F0;
	cursor:pointer;
	padding-left:5px;
	padding-right:4em;
	line-height:1.5;
	font-size:0.85rem;
	overflow:hidden;
}

.ATAWindow .Content {
	width:100%;
	left:0;
	position:absolute;
	padding:3px;
	padding-left:5px;
	font-size:0.85rem;
	border-right:5px solid #00000000;
}

.ATAWindow .Actions {
	position:absolute;
	font-size:0.9em;
	top:0;
	right:5px;
}

.ATAWindow .Lock {
	position:absolute;
	top:0;
	right:0;
	left:0;
	bottom:0;
	z-index:1000;
	width:100%;
	height:100%;
	margin:0;
	padding:0;
	background-color:#FFFFFF01;
	backdrop-filter:blur(0.1px);
}

.ATAWindow .Edge_Right {
	position:absolute;
	background-color:inherit;
	opacity:0.5;
	width:5px;
	height:100%;
	top:0;
	right:0;
	cursor:e-resize;
}

.ATAWindow .Edge_Left {
	position:absolute;
	background-color:inherit;
	opacity:0.5;
	width:5px;
	height:100%;
	bottom:0;
	left:0;
	cursor:e-resize;
}

.ATAWindow .Edge_Bottom {
	position:absolute;
	background-color:inherit;
	opacity:0.5;
	width:100%;
	height:5px;
	bottom:0;
	left:0;
	cursor:n-resize;
}

