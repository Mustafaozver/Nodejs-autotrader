WalletWindow.Windows = {};
WalletWindow.prototype.Serialize = function(){
	var elePos = this.WIN.GetPosition();
	var eleSize = this.WIN.GetSize();
	var xcode = "(function(){";
	xcode += "var _=IdealInterFace.Splitage(\"" + this.Symbol + "\");";
	xcode += "setTimeout(function(){"
	xcode += 	"_.WIN.SetPosition(" + elePos.x + "," + elePos.y + ");";
	xcode += 	"_.WIN.SetSize(" + eleSize.w + "," + eleSize.h + ");";
	xcode += "},1);";
	xcode += "})();";
	return xcode;
};
WalletWindow.Serialize = function(){
	var xcode = "(function(){";
	//xcode += "WalletWindow.Config.=" + WalletWindow.Config. + ";";
	for(var key in WalletWindow.Windows){
		xcode += WalletWindow.Windows[key].Serialize();
	}
	xcode += "})();";
	return xcode;
};
WalletWindow.Deserialize = function(xcode){
	if(typeof(xcode)=="string")setTimeout(function(){eval(xcode)},1);
	else if(typeof(xcode)=="function")setTimeout(xcode,1);
};
WalletWindow.Reset = function(){
	for(var key in WalletWindow.Windows){
		WalletWindow.Windows[key].Close();
	}
};
WalletWindow.prototype.Close = function(){
	//WalletWindow.Windows[this.ID].WIN.Close();
	delete WalletWindow.Windows[this.ID];
};
